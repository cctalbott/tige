package com.build.main {	import flash.display.Sprite;	import flash.display.MovieClip;	import flash.display.Shape;	import flash.text.TextField;	import ContactBtnMCClass;	import com.build.main.ContactFormClass;	import flash.events.Event;	import flash.events.MouseEvent;	import flash.geom.Transform;	import flash.geom.ColorTransform;		public class ContactBuildClass extends Sprite {		private var _contactBtn:ContactBtnMCClass = new ContactBtnMCClass();		private var _contactForm:ContactFormClass = new ContactFormClass();				public function ContactBuildClass():void {			_contactForm.name = "_contactForm";			_contactBtn.x = 18;			_contactBtn.y = 397;			addChild(_contactBtn);			_contactBtn.addEventListener(Event.ADDED_TO_STAGE, contactBtnAdded, false, 0, true);		}		private function contactBtnAdded(event:Event):void {			_contactBtn.buttonMode = true;			TextField(_contactBtn.getChildAt(2)).mouseEnabled = false;			_contactBtn.addEventListener(MouseEvent.MOUSE_OVER, contactBtnOver, false, 0, true);			_contactBtn.addEventListener(MouseEvent.MOUSE_OUT, contactBtnOut, false, 0, true);			_contactBtn.addEventListener(MouseEvent.CLICK, contactBtnClicked, false, 0, true);						_contactBtn.removeEventListener(Event.ADDED_TO_STAGE, contactBtnAdded, false);		}		private function contactBtnOver(event:MouseEvent):void {			//contact bg #666			var contactBg:Shape = Shape(MovieClip(_contactBtn.getChildAt(0)).getChildAt(0));			var colorInfo:ColorTransform = contactBg.transform.colorTransform;			colorInfo.color = 0x666666;			contactBg.transform.colorTransform = colorInfo;			//contact txt #FFF			var contactTxt:TextField = TextField(_contactBtn.getChildAt(2));			colorInfo = contactTxt.transform.colorTransform;			colorInfo.color = 0xFFFFFF;			contactTxt.transform.colorTransform = colorInfo;		}		private function contactBtnOut(event:MouseEvent):void {			//contact bg #E0E0E0			var contactBg:Shape = Shape(MovieClip(_contactBtn.getChildAt(0)).getChildAt(0));			var colorInfo:ColorTransform = contactBg.transform.colorTransform;			colorInfo.color = 0xE0E0E0;			contactBg.transform.colorTransform = colorInfo;			//contact txt #141414			var contactTxt:TextField = TextField(_contactBtn.getChildAt(2));			colorInfo = contactTxt.transform.colorTransform;			colorInfo.color = 0x141414;			contactTxt.transform.colorTransform = colorInfo;		}		private function contactBtnClicked(event:MouseEvent):void {			addChild(_contactForm);		}	}}