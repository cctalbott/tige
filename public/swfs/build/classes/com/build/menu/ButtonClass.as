package com.build.menu {	import flash.display.Sprite;	import flash.events.MouseEvent;		public class ButtonClass extends Sprite {		public function ButtonClass(theButton:Object, theClickFn:Object, theOverFn:Object, theOutFn:Object) 		{			theButton.buttonMode = true;						theButton.addEventListener(MouseEvent.CLICK, theClickFn);			theButton.addEventListener(MouseEvent.MOUSE_OVER, theOverFn);			theButton.addEventListener(MouseEvent.MOUSE_OUT, theOutFn);		}	}}