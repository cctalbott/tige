package com.build.menu {	import flash.display.Sprite;	import flash.display.Shape;	import flash.display.CapsStyle;	import flash.display.JointStyle;	import flash.display.LineScaleMode;	import flash.events.Event;		public class SectionMenuLinesClass extends Sprite {		private var _lines:Sprite = new Sprite();		private var _linetop:Shape = new Shape();		private var _lineexterior:Shape = new Shape();		private var _linegraphic:Shape = new Shape();		private var _lineinterior:Shape = new Shape();				public function SectionMenuLinesClass() {			init();		}				private function init():void {			addChild(_lines);			_lines.addEventListener(Event.ADDED_TO_STAGE, linesAdded);		}				private function linesAdded(event:Event):void 		{		  var mainTimeline:Object = Object(_lines.parent.parent.parent);		  			_linetop.graphics.lineStyle(1, 0x767676, 1);			_lineexterior.graphics.lineStyle(1, 0x767676, 1);			_linegraphic.graphics.lineStyle(1, 0x767676, 1);			_lineinterior.graphics.lineStyle(1, 0x767676, 1);						// top line		  _linetop.graphics.lineTo(201, 0);		  			if(mainTimeline._model == "20V" || mainTimeline._model == "20i")			{        // exterior bottom  			_lineexterior.graphics.moveTo(0, 145);  			_lineexterior.graphics.lineTo(201, 145);        // graphics bottom  			_linegraphic.graphics.moveTo(0, 183);  			_linegraphic.graphics.lineTo(201, 183);        // interior bottom  			_lineinterior.graphics.moveTo(0, 221);  			_lineinterior.graphics.lineTo(201, 221);			}			else if(mainTimeline._model == "RZR")			{			  // exterior bottom  			_lineexterior.graphics.moveTo(0, 195);  			_lineexterior.graphics.lineTo(201, 195);        // graphics bottom  			_linegraphic.graphics.moveTo(0, 233);  			_linegraphic.graphics.lineTo(201, 233);        // interior bottom  			_lineinterior.graphics.moveTo(0, 271);  			_lineinterior.graphics.lineTo(201, 271);			}			else if(mainTimeline._model == "R20")			{			  // exterior bottom  			_lineexterior.graphics.moveTo(0, 195);  			_lineexterior.graphics.lineTo(201, 195);        // graphics bottom  			_linegraphic.graphics.moveTo(0, 233);  			_linegraphic.graphics.lineTo(201, 233);        // interior bottom  			_lineinterior.graphics.moveTo(0, 271);  			_lineinterior.graphics.lineTo(201, 271);			}			else			{			  // exterior bottom  			_lineexterior.graphics.moveTo(0, 165);  			_lineexterior.graphics.lineTo(201, 165);        // graphics bottom  			_linegraphic.graphics.moveTo(0, 203);  			_linegraphic.graphics.lineTo(201, 203);        // interior bottom  			_lineinterior.graphics.moveTo(0, 241);  			_lineinterior.graphics.lineTo(201, 241);			}						_lines.addChild(_linetop);			_lines.addChild(_lineexterior);			_lines.addChild(_linegraphic);			_lines.addChild(_lineinterior);		}	}}