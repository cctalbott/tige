package com.build.menu {	import flash.display.Sprite;	import flash.events.Event;	import com.build.menu.SectionMenuLinesClass;	import com.build.menu.SectionMenuMainBtnClass;	import com.build.menu.SectionMenuSecondaryBtnClass;		public class SectionMenuClass extends Sprite {		public var _exteriorSections:Sprite = new Sprite();		public var _interiorSections:Sprite = new Sprite();		private var _lines:SectionMenuLinesClass = new SectionMenuLinesClass();		private var _exterior:SectionMenuMainBtnClass = new SectionMenuMainBtnClass("1. EXTERIOR");		private var _deck:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("deck: ");		private var _deckAccent:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("deck accent: ");		private var _splitDeckAccent:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("split deck accent: ");		private var _hull:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("hull: ");		private var _mainPanel:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("main panel: ");		private var _mainPanelAccent:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("main panel accent: ");		private var _secondPanel:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("second panel: ");		private var _secondPanelAccent:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("second panel accent: ");		private var _transomAccent:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("transom accent: ");		private var _swimPlatform:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("swim platform: ");		private var _tower:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("tower: ");		private var _graphic:SectionMenuMainBtnClass = new SectionMenuMainBtnClass("2. GRAPHIC");		private var _interior:SectionMenuMainBtnClass = new SectionMenuMainBtnClass("3. INTERIOR");		private var _baseInterior:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("base interior: ");		private var _accentVinyl:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("piping color: ");		private var _accentTexture:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("accent vinyl: ");		private var _dashAccent:SectionMenuSecondaryBtnClass = new SectionMenuSecondaryBtnClass("dash accent: ");				public function SectionMenuClass() 		{			init();		}				private function init():void 		{ 		  // set name			_exterior.name = "_exterior";			_exteriorSections.name = "_exteriorSections";			_deck.name = "_deck";			_deckAccent.name = "_deckAccent";			_splitDeckAccent.name = "_splitDeckAccent";			_hull.name = "_hull";			_mainPanel.name = "_mainPanel";			_mainPanelAccent.name = "_mainPanelAccent";			_secondPanel.name = "_secondPanel";			_secondPanelAccent.name = "_secondPanelAccent";			_transomAccent.name = "_transomAccent";			_swimPlatform.name = "_swimPlatform";			_tower.name = "_tower";			_graphic.name = "_graphic";			_interior.name = "_interior";			_interiorSections.name = "_interiorSections";			_baseInterior.name = "_baseInterior";			_accentVinyl.name = "_accentVinyl";			_accentTexture.name = "_accentTexture";			_dashAccent.name = "_dashAccent";			_lines.name = "_lines";						// add to menu			addChild(_exterior);			addChild(_exteriorSections);			addChild(_graphic);			addChild(_interior);			addChild(_interiorSections);			addChild(_lines);						_interiorSections.visible = false;			_exteriorSections.addEventListener(Event.ADDED_TO_STAGE, exteriorSectionsAdded, false, 0, true);			_interiorSections.addEventListener(Event.ADDED_TO_STAGE, interiorSectionsAdded, false, 0, true);		}				private function exteriorSectionsAdded(event:Event):void 		{		  var mainTimeline:Object = Object(_exteriorSections.parent.parent);		  		  // set position		  _exteriorSections.x = 24;		  _exteriorSections.y = 27;			_interiorSections.x = 24;						// y + 20 if tower color options			if(mainTimeline._model == "RZR")			{  			_interiorSections.y = 103;  			_graphic.y = 195;  			_interior.y = 233;			}			else if(mainTimeline._model == "R20")			{			  _interiorSections.y = 103;  			_graphic.y = 195;  			_interior.y = 233;			}			else if(mainTimeline._model == "20V" && mainTimeline._model == "20i")			{  			_interiorSections.y = 103;  			_graphic.y = 145;  			_interior.y = 183;		  }		  else		  {  			_interiorSections.y = 123;  			_graphic.y = 165;  			_interior.y = 203;		  }		  		  // add to menu			_exteriorSections.addChild(_deck);						if(mainTimeline._model == "RZ4" || mainTimeline._model == "RZ2" || mainTimeline._model == "RZR" || mainTimeline._model == "R20" || mainTimeline._model == "24Vé" && mainTimeline._vescheme == "flex" || mainTimeline._model == "22Vé" && mainTimeline._vescheme == "flex" || mainTimeline._model == "Z1" || mainTimeline._model == "22i" && mainTimeline._vescheme == "flex" || mainTimeline._model == "20V" && mainTimeline._vescheme == "flex" || mainTimeline._model == "20i" && mainTimeline._vescheme == "flex") 			{				_exteriorSections.addChild(_deckAccent);			}						if(mainTimeline._model == "RZ4" || mainTimeline._model == "RZ2" || mainTimeline._model == "RZR" || mainTimeline._model == "Z1") 			{				_exteriorSections.addChild(_splitDeckAccent);			}						_exteriorSections.addChild(_hull);			_exteriorSections.addChild(_mainPanel);						if(mainTimeline._model == "RZR" || mainTimeline._model == "R20")			{			  _exteriorSections.addChild(_mainPanelAccent);			}						if(mainTimeline._model !== "Z1" && mainTimeline._model !== "R20") 			{				_exteriorSections.addChild(_secondPanel);			}						if(mainTimeline._model == "RZR")			{			  _exteriorSections.addChild(_secondPanelAccent);			}						if(mainTimeline._model == "Z1")			{			  _exteriorSections.addChild(_transomAccent);			}						_exteriorSections.addChild(_swimPlatform);						// set position			_deckAccent.y = _deck.y + _deck.height;			_splitDeckAccent.y = _deckAccent.y + _deckAccent.height;			_hull.y = _splitDeckAccent.y + _splitDeckAccent.height;			_mainPanel.y = _hull.y + _hull.height;						if(mainTimeline._model == "RZR" || mainTimeline._model == "R20")			{			  _mainPanelAccent.y = _mainPanel.y + _mainPanel.height;			}						if(mainTimeline._model !== "Z1" && mainTimeline._model !== "R20")			{			  if(mainTimeline._model == "RZR")			  {			    _secondPanel.y = _mainPanelAccent.y + _mainPanelAccent.height;			    _secondPanelAccent.y = _secondPanel.y + _secondPanel.height;			    _swimPlatform.y = _secondPanelAccent.y + _secondPanelAccent.height;			  }			  else			  {			    _secondPanel.y = _mainPanel.y + _mainPanel.height;			    _swimPlatform.y = _secondPanel.y + _secondPanel.height;			  }			}			else			{			  if(mainTimeline._model == "Z1")			  {			    _transomAccent.y = _mainPanel.y + _mainPanel.height;			    _swimPlatform.y = _transomAccent.y + _transomAccent.height;			  }			  else 			  {			    _swimPlatform.y = _mainPanelAccent.y + _mainPanelAccent.height;			  }			}						// tower			if(mainTimeline._model !== "20V" && mainTimeline._model !== "20i" && mainTimeline._model !== "R20")			{			  _exteriorSections.addChild(_tower);			  _tower.y = _swimPlatform.y + _swimPlatform.height;			}						_exteriorSections.removeEventListener(Event.ADDED_TO_STAGE, exteriorSectionsAdded, false);		}				private function interiorSectionsAdded(event:Event):void 		{			var mainTimeline:Object = Object(_interiorSections.parent.parent);						_interiorSections.addChild(_baseInterior);			_interiorSections.addChild(_accentVinyl);			_interiorSections.addChild(_accentTexture);						if(mainTimeline._model !== "20i" && mainTimeline._model !== "20V" && mainTimeline._model !== "22i") 			{				_interiorSections.addChild(_dashAccent);			}						_accentVinyl.y = _baseInterior.y + _baseInterior.height;			_accentTexture.y = _accentVinyl.y + _accentVinyl.height;						if(mainTimeline._model !== "20i" && mainTimeline._model !== "20V" && mainTimeline._model !== "22i") 			{				_dashAccent.y = _accentTexture.y + _accentTexture.height;			}						_interiorSections.removeEventListener(Event.ADDED_TO_STAGE, interiorSectionsAdded, false);		}	}}